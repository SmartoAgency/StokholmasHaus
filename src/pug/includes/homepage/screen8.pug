- 
    var apartments = [
        {
            name: "apartment-button",
            title: "Stokholmas 26",
            value: "stokholmas-26",
            id: "stokholmas-26",
            dataBuild: 1,
        },
        {
            name: "apartment-button",
            title: "Stokholmas 26 c",
            value: "stokholmas-26-c",
            id: "stokholmas-26_c",
            dataBuild: 2,
        },
    ];

section#plans.home-screen8.screen-home(data-apartment-section)
    .home-screen8-row.container
        ul.home-screen8-buttons(container-button-scheme)
            each apartment in apartments
                li.home-screen8-buttons__item
                    button(value= apartment.value data-build=apartment.dataBuild).label-button= apartment.title
        .home-screen8-apartment(data-apartment-info)
            .apartment-info(data-apartment-info-table)
                table
                    thead
                        tr
                            td Общая площадь:
                            //- td(data-apartment-label="currency") 154.6 м2
                            td
                                span.table-row-spinner.spinner
                        tr
                            td Количество комнат:
                            //- td(data-apartment-label="all_room") 4
                            td
                                span.table-row-spinner.spinner
                    tbody
                        tr
                            td(style="width: 100%; height: 100%; display: flex; align-items: center; justify-content: center;")
                                span.table-row-spinner.spinner
                        //- tr 
                        //-     td Кухня:
                        //-     td(data-apartment-label="custom_field_1") 32.8 м2
