                    button(value=1 data-apartment-level data-section=1) 1
                    button(value=2 data-apartment-level data-section=2).apartment-level-btn__active 2 
    .home-screen8-img
        .home-screen8-img__badge-one
            include ../homePageElements/pin.pug
            +badge('Stokholmas 26 A')
        +img('home/home-page-screen8.png')
